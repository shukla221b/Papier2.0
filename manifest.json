{
  "manifest_version": 3,
  "name": "Styled Notepad",
  "version": "1.0",
  "description": "A minimalist, distraction-free notepad for your new tab.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [],
  "action": {
    "default_icon": "icon.png",
    "default_title": "Open Notepad"
  }
}
